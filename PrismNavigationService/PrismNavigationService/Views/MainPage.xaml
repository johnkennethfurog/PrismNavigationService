<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PrismNavigationService.Views.MainPage"
             Title="{Binding Title}">

    <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
        <Button Text="Go to Custom Layout using Prism Nav Service" Command="{Binding GotoCustLayout}"/>
        <Button Text="Go to Custom Layout using Navigation" x:Name="btnNav"/>
    </StackLayout>

</ContentPage>